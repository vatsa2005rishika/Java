interface Drawable {
    void draw();
}

class Rectangle implements Drawable {
    public void draw() {
        System.out.println("drawing rectangle");
    }
}

class Circle implements Drawable {
    public void draw() {
        System.out.println("drawing circle");
    }
}

class Main {
    public static void main(String[] args) {
        Drawable d = new Circle();  // Corrected class name (Circle)
        d.draw();
        
        Drawable d1 = new Rectangle();  // Corrected class name (Rectangle)
        d1.draw();
    }
}
