class Shape {
    double ar1;

    // Constructor for triangle
    public Shape(int a, int b) {
        ar1 = (a * b) / 2.0;
        System.out.println("Area of triangle = " + ar1);
    }

    // Constructor for circle
    public Shape(float a) {
        ar1 = Math.PI * a * a;
        System.out.println("Area of circle = " + ar1);
    }

    // Constructor for square
    public Shape(int a) {
        ar1 = a * a;
        System.out.println("Area of square = " + ar1);
    }
}

class Main {
    public static void main(String [] args) {
        Shape S1 = new Shape(5, 6);  // Triangle
        Shape S2 = new Shape(2.4f);  // Circle
        Shape S3 = new Shape(3);     // Square
    }
}
